ll-group>\n                </div>\n                <van-divider style=\"margin: 3px\"/>\n                <div class=\"login-box-item\">\n                    <van-cell-group>\n                        <van-field v-model=\"form.password\" :border=\"false\" type=\"password\" placeholder=\"Password\"/>\n                    </van-cell-group>\n                </div>\n            </div>\n            <van-button\n                    @click=\"submit\"\n                    class=\"login-box-btn\"\n                    round type=\"info\"\n                    color=\"linear-gradient(to right, #4bb0ff, #6149f6)\">\n                <b>Login</b>\n            </van-button>\n            <div class=\"login-box-register\">\n                <router-link to=\"/register\" style=\"color: #888888\">还没有账号?</router-link>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import {checkLoginForm} from \"../../utils/check\";\n    import {userLogin} from \"../../api/user\";\n\n    export default {\n        name: \"index\",\n        data() {\n            return {\n                form: {\n                    mobile: '',\n                    password: ''\n                },\n            }\n        },\n\n        methods: {\n            submit() {\n                if (checkLoginForm(this.form).valueOf()) {\n\n                    //登录逻辑\n                    userLogin(this.form).then((res) => {\n                        if (res.status === true) {\n                            const user = res.data.user\n                            //保存登录信息\n                            localStorage.setItem(\"token\", user.id)\n                            this.$router.push('/')\n                        }\n                    })\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    @import \"../../assets/css/login.css\";\n\n</style>"]}]}